# TODO: Translation updated at 2023-07-25 15:55 #|#|# # game/scripts/quests/questSecretOfTheSilverDragon/mini_games/mini_game.rpy:168 #-#-# translate crowdin gameower_410261a5:
msgid "[_chenlin_] Д-джоан? Э-это ты.."
msgstr "[_chenlin_] D-Joan? Sei tu."

# game/scripts/quests/questSecretOfTheSilverDragon/mini_games/mini_game.rpy:169 #-#-# translate crowdin gameower_1521759a:
msgid "[_chenlin_] Ч-черт, я так надеялся, что это Хай Лин.."
msgstr "[_chenlin_] Dannazione, speravo che fosse Hay Lin."

# game/scripts/quests/questSecretOfTheSilverDragon/mini_games/mini_game.rpy:170 #-#-# translate crowdin gameower_fecdf984:
msgid "[_chenlin_] Поэтому игнорировал звуки в темноте.."
msgstr "[_chenlin_] Così ho ignorato i suoni nel buio."

# game/scripts/quests/questSecretOfTheSilverDragon/mini_games/mini_game.rpy:171 #-#-# translate crowdin gameower_137584af:
msgid "[_chenlin_] Н-но.. При свете холодильника все стало видно.."
msgstr "[_chenlin_] Ma... Con la luce del frigorifero si vede tutto."

# game/scripts/quests/questSecretOfTheSilverDragon/mini_games/mini_game.rpy:172 #-#-# translate crowdin gameower_d7dc8529:
msgid "[_chenlin_] К-как ты могла, Джоан.."
msgstr "[_chenlin_] Come hai potuto, Joan?"

# game/scripts/quests/questSecretOfTheSilverDragon/mini_games/mini_game.rpy:173 #-#-# translate crowdin gameower_4134acaf:
msgid "[_chenlin_] П-почему.."
msgstr "[_chenlin_] P-perché..."

# game/scripts/quests/questSecretOfTheSilverDragon/mini_games/mini_game.rpy:180 #-#-# translate crowdin gameower_52ba0c08:
msgid "[_johnlin_] Н-нет, Чен, подожди.."
msgstr "[_johnlin_] N-no, Chen, aspetta."

# game/scripts/quests/questSecretOfTheSilverDragon/mini_games/mini_game.rpy:181 #-#-# translate crowdin gameower_5f4f1ad3:
msgid "[_cedrick_] (Черт, это рушит абсолютно все мои планы касательно Хай Лин.. Такую ситуацию я уже решить не смогу..)"
msgstr "[_cedrick_] (Dannazione, questo rovina assolutamente tutti i miei piani per Hay Lin.... Questa è una situazione che non riesco a risolvere...)"

# game/scripts/quests/questSecretOfTheSilverDragon/mini_games/mini_game.rpy:182 #-#-# translate crowdin gameower_96fbd5fa:
msgid "[_cedrick_] (Для моих отношений с Хай Лин это конец..)"
msgstr "[_cedrick_] (Per quanto riguarda la mia relazione con Hay Lin, questa è la fine...)."

# game/scripts/quests/questSecretOfTheSilverDragon/mini_games/mini_game.rpy:201 #-#-# translate crowdin gamewin_d4fee99f:
msgid "[_cedrick_] Ч-черт, я кончаю.."
msgstr "[_cedrick_] Merda, sto arrivando."

# game/scripts/quests/questSecretOfTheSilverDragon/mini_games/mini_game.rpy:202 #-#-# translate crowdin gamewin_db06404e:
msgid "[_johnlin_] Ааахх.. Д-давай.. Я приму все в себя.. Не сдерживайся.."
msgstr "[_johnlin_] Aaahhhh. Vai avanti. Lo prendo tutto io. Non si trattenga."

# game/scripts/quests/questSecretOfTheSilverDragon/mini_games/mini_game.rpy:203 #-#-# translate crowdin gamewin_d176364d:
msgid "[_cedrick_] Т-тогда получай!!"
msgstr "[_cedrick_] Allora prendilo!!!"

# game/scripts/quests/questSecretOfTheSilverDragon/mini_games/mini_game.rpy:208 #-#-# translate crowdin gamewin_4efee3a5:
msgid "[_johnlin_] Аааааахххх…"
msgstr "[_johnlin_] Aaaaaaahhhhh…"

# game/scripts/quests/questSecretOfTheSilverDragon/mini_games/mini_game.rpy:209 #-#-# translate crowdin gamewin_e3a6ae4d:
msgid "[_johnlin_] Т-так горячо внутри.. К-как же хорошо.. Обжигающе горячо.."
msgstr "[_johnlin_] E' così caldo all'interno. H-quanto è buono. È bollente."

# game/scripts/quests/questSecretOfTheSilverDragon/mini_games/mini_game.rpy:210 #-#-# translate crowdin gamewin_a20cefa7:
msgid "..."
msgstr "..."

# game/scripts/quests/questSecretOfTheSilverDragon/mini_games/mini_game.rpy:211 #-#-# translate crowdin gamewin_07bdde05:
msgid "....."
msgstr "....."

# game/scripts/quests/questSecretOfTheSilverDragon/mini_games/mini_game.rpy:212 #-#-# translate crowdin gamewin_d1ddaffa:
msgid "........"
msgstr "........"

# game/scripts/quests/questSecretOfTheSilverDragon/mini_games/mini_game.rpy:213 #-#-# translate crowdin gamewin_391115bb:
msgid "[_cedrick_] И что мы теперь будем делать?"
msgstr "[_cedrick_] Cosa faremo adesso?"

# game/scripts/quests/questSecretOfTheSilverDragon/mini_games/mini_game.rpy:215 #-#-# translate crowdin gamewin_6a3a8fa9:
msgid "[_johnlin_] ..."
msgstr "[_johnlin_] ..."

# game/scripts/quests/questSecretOfTheSilverDragon/mini_games/mini_game.rpy:216 #-#-# translate crowdin gamewin_cc4385d8:
msgid "[_johnlin_] Просто подождем, пока он уйдет.."
msgstr "[_johnlin_] Aspettiamo che se ne vada."

# game/scripts/quests/questSecretOfTheSilverDragon/mini_games/mini_game.rpy:218 #-#-# translate crowdin gamewin_72684a29:
msgid "Чен уходит в туалет. В это время Джоан возвращается в спальню. Седрик наконец ложится спать"
msgstr "Chen esce per andare in bagno. In quel momento Joan torna in camera da letto. Cedric va finalmente a letto"

# game/scripts/quests/questSecretOfTheSilverDragon/mini_games/mini_game.rpy:219 #-#-# translate crowdin gamewin_a20cefa7_1:
msgid "..."
msgstr "..."

# game/scripts/quests/questSecretOfTheSilverDragon/mini_games/mini_game.rpy:220 #-#-# translate crowdin gamewin_5f9b772e:
msgid "...."
msgstr "...."

# game/scripts/quests/questSecretOfTheSilverDragon/mini_games/mini_game.rpy:221 #-#-# translate crowdin gamewin_0fd4d656:
msgid "......"
msgstr "......"

# game/scripts/quests/questSecretOfTheSilverDragon/mini_games/mini_game.rpy:238 #-#-# translate crowdin d_01_c451911c:
msgid "[_johnlin_] Р-рик.. Я, ах, я не могу.."
msgstr "[_johnlin_] R-rick. Io... non posso."

# game/scripts/quests/questSecretOfTheSilverDragon/mini_games/mini_game.rpy:239 #-#-# translate crowdin d_01_adf49ede:
msgid "[_johnlin_] Д-дай мне больше.. Хочу почувствовать его внутри себя.."
msgstr "[_johnlin_] Dammi di più. Voglio sentirlo dentro di me."

# game/scripts/quests/questSecretOfTheSilverDragon/mini_games/mini_game.rpy:240 #-#-# translate crowdin d_01_37c25ec0:
msgid "[_cedrick_] Ч-черт, думаешь стоит?"
msgstr "[_cedrick_] Merda, pensi che dovrei?"

# game/scripts/quests/questSecretOfTheSilverDragon/mini_games/mini_game.rpy:241 #-#-# translate crowdin d_01_9da55ec7:
msgid "[_johnlin_] М-мне плевать.. Я.."
msgstr "[_johnlin_] Non mi interessa. Я.."

# game/scripts/quests/questSecretOfTheSilverDragon/mini_games/mini_game.rpy:242 #-#-# translate crowdin d_01_e2563cc8:
msgid "[_johnlin_] Я просто хочу этого.."
msgstr "[_johnlin_] Lo voglio e basta."

# game/scripts/quests/questSecretOfTheSilverDragon/mini_games/mini_game.rpy:243 #-#-# translate crowdin d_01_bc5704e7:
msgid "[_cedrick_] Х-хорошо.."
msgstr "[_cedrick_] Va bene."

# game/scripts/quests/questSecretOfTheSilverDragon/mini_games/mini_game.rpy:244 #-#-# translate crowdin d_01_080f10af:
msgid "[_cedrick_] (Думаю лучше не стоит называть ее по имени. Даже если Чен и слышит это все, то пока виду не подает)"
msgstr "[_cedrick_] (Penso che sia meglio non chiamarla per nome. Anche se Chen sente tutto questo, non lo dà ancora a vedere)."

# game/scripts/quests/questSecretOfTheSilverDragon/mini_games/mini_game.rpy:245 #-#-# translate crowdin d_01_b3a30d87:
msgid "[_cedrick_] (Возможно думает, что это Хай Лин и не хочет быть инициатором неловкой сцены..)"
msgstr "[_cedrick_] (Probabilmente pensa che sia Hay Lin e non vuole essere l'istigatore di una scena imbarazzante...)."

# game/scripts/quests/questSecretOfTheSilverDragon/mini_games/mini_game.rpy:246 #-#-# translate crowdin d_01_7b5009ba:
msgid "[_cedrick_] (Все же застукать и попытаться остановить свою дочь во время секса - дело такое)"
msgstr "[_cedrick_] (Tuttavia, catturare e cercare di impedire a tua figlia di fare sesso è una cosa che si fa)."

# game/scripts/quests/questSecretOfTheSilverDragon/mini_games/mini_game.rpy:247 #-#-# translate crowdin d_01_04619522:
msgid "[_cedrick_] (Но вот если он поймет, что это Джоан...)"
msgstr "[_cedrick_] (Ma se si accorge che è Joan...)."

# game/scripts/quests/questSecretOfTheSilverDragon/mini_games/mini_game.rpy:277 #-#-# translate crowdin d_02_3ae3cf21:
msgid "[_cedrick_] Ах ты чертовка.."
msgstr "[_cedrick_] Piccolo diavolo."

# game/scripts/quests/questSecretOfTheSilverDragon/mini_games/mini_game.rpy:278 #-#-# translate crowdin d_02_207b80dd:
msgid "[_cedrick_] Ууууххх.. Всерьез меня разгоячила.."
msgstr "[_cedrick_] Oohhh. Mi hai fatto davvero agitare."

# game/scripts/quests/questSecretOfTheSilverDragon/mini_games/mini_game.rpy:279 #-#-# translate crowdin d_02_1e5e45c5:
msgid "[_cedrick_] Осознаешь последствия своих действий?"
msgstr "[_cedrick_] Siete consapevoli delle conseguenze delle vostre azioni?"

# game/scripts/quests/questSecretOfTheSilverDragon/mini_games/mini_game.rpy:280 #-#-# translate crowdin d_02_af1554d7:
msgid "[_johnlin_] Мммввв.. Да, дай мне почувствовать всю твою яростную мощь"
msgstr "[_johnlin_] Mmmwww. Sì, fammi sentire tutto il tuo potere feroce."

# game/scripts/quests/questSecretOfTheSilverDragon/mini_games/mini_game.rpy:281 #-#-# translate crowdin d_02_7380dec0:
msgid "[_cedrick_] Ложись на диван, сейчас ты почувствуешь его по самую матку!"
msgstr "[_cedrick_] Sdraiati sul divano, lo sentirai fino all'utero!"

# game/scripts/quests/questSecretOfTheSilverDragon/mini_games/mini_game.rpy:282 #-#-# translate crowdin d_02_d557226e:
msgid "[_johnlin_] Ооо дааа"
msgstr "[_johnlin_] Oh, sì."

