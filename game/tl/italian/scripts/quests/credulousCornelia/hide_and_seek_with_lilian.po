# TODO: Translation updated at 2023-07-25 15:55 #|#|# # game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:7 #-#-# translate crowdin hide_and_seek_with_lilian_471e6c98:
msgid "[_cedrick_] Твоя сестра на Меридиане.."
msgstr "[_cedrick_] Tua sorella sulla Meridian..."

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:11 #-#-# translate crowdin hide_and_seek_with_lilian_b4918782:
msgid "[_cornelia_] Меридиане? Откуда ты.."
msgstr "[_cornelia_] Meridian? Dove hai..."

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:12 #-#-# translate crowdin hide_and_seek_with_lilian_935f99a5:
msgid "[_cedrick_] Дело в том, что я – Лорд Седрик! Правая рука князя Фобоса"
msgstr "[_cedrick_] Il fatto è che io sono Lord Cedric! Il braccio destro del principe Phobos."

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:17 #-#-# translate crowdin hide_and_seek_with_lilian_d5226c1f:
msgid "[_cornelia_] Что!? Н-но.. как это возможно!?"
msgstr "[_cornelia_] Cosa? Ma... com'è possibile!?"

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:18 #-#-# translate crowdin hide_and_seek_with_lilian_ee87dbc4:
msgid "[_cedrick_] Твоей сестре не очень нравилось, как ты с ней обращалась, и я предложил ей отправиться со мной на Меридиан"
msgstr "[_cedrick_] A tua sorella non piaceva il modo in cui la trattavi, così le ho suggerito di venire con me a Meridian."

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:22 #-#-# translate crowdin hide_and_seek_with_lilian_77c76886:
msgid "[_cornelia_] Р-Рик.. Как ты мог, мы же.. Ах ты.. Ублюдок!"
msgstr "[_cornelia_] R-Rick... Come hai potuto, noi... Tu... Bastardo!"

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:24 #-#-# translate crowdin hide_and_seek_with_lilian_1b011c18:
msgid "[_cornelia_] Т-ты ее и пальцем не посмеешь тронуть, понял!?"
msgstr "[_cornelia_] Non la toccherai con un dito, capito?!"

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:28 #-#-# translate crowdin hide_and_seek_with_lilian_e9c3f58d:
msgid "[_cedrick_] Не тебе здесь ставить условия, дорогая. Пока что к твоей сестре хорошо относятся, но будешь мне хамить, и мои стражники будут уже не так добры с ней"
msgstr "[_cedrick_] Non sta a te stabilire le condizioni qui, mia cara. Finora tua sorella è stata trattata bene, ma se sei scortese con me, le mie guardie non saranno altrettanto gentili con lei\"."

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:32 #-#-# translate crowdin hide_and_seek_with_lilian_9f0a825e:
msgid "[_cornelia_] Н-не надо.."
msgstr "[_cornelia_] Non..."

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:33 #-#-# translate crowdin hide_and_seek_with_lilian_9939e51c:
msgid "[_cedrick_] (Но, если я неправильно на нее надавлю, она может вместо того, чтобы преследовать меня, может сначала побежать к подругам за помощью, тогда мой план провалится..)"
msgstr "[_cedrick_] (Ma se la spingo a sbagliare, potrebbe correre a chiedere aiuto ai suoi amici invece di inseguirmi, e allora il mio piano fallirebbe...)"

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:34 #-#-# translate crowdin hide_and_seek_with_lilian_10f8308a:
msgid "[_cedrick_] (Хм.. Есть один способ.. Непреложный обет.. Я не очень-то хорош в такого типа магии, это скорее специализация Фобоса, но другого выхода в этой ситуации у меня не остается)"
msgstr "[_cedrick_] (Hmm... C'è un modo... Un voto infrangibile... Non sono molto bravo in questo tipo di magia, è più una specialità di Phobos, ma non ho altra scelta in questa situazione)."

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:35 #-#-# translate crowdin hide_and_seek_with_lilian_62e7ee86:
msgid "[_cornelia_] Я сделаю все, только не троньте мою сестру.."
msgstr "[_cornelia_] Farò qualsiasi cosa, ma non toccare mia sorella..."

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:39 #-#-# translate crowdin hide_and_seek_with_lilian_3857a2a9:
msgid "[_cedrick_] С твоей сестрой ничего не случится, если ты дашь слово, что никому не расскажешь ни о тайне моей личности, ни о том, как пропала Лилиан"
msgstr "[_cedrick_] Non accadrà nulla a tua sorella se darai la tua parola di non rivelare a nessuno il segreto della mia identità e della scomparsa di Lilian."

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:43 #-#-# translate crowdin hide_and_seek_with_lilian_a97c1d75:
msgid "[_cornelia_] Я.. Я никому не скажу, честное слово! Я клянусь!"
msgstr "[_cornelia_] Я.. Non lo dirò a nessuno, lo giuro! Lo giuro!"

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:47 #-#-# translate crowdin hide_and_seek_with_lilian_d9c97c79:
msgid "[_cedrick_] Я бы рад поверить тебе на слово, но этого недостаточно"
msgstr "[_cedrick_] Mi piacerebbe crederle sulla parola, ma non è sufficiente."

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:48 #-#-# translate crowdin hide_and_seek_with_lilian_ca3ba1d6:
msgid "[_cedrick_] Я хочу, чтобы ты дала клятву.. Непреложный обет.."
msgstr "[_cedrick_] Voglio che tu faccia un giuramento... Un voto infrangibile..."

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:52 #-#-# translate crowdin hide_and_seek_with_lilian_3f1624c7:
msgid "[_cornelia_] Ты.. Да как ты только можешь, после всего, что между нами было..."
msgstr "[_cornelia_] Tu, uh... Come hai potuto, dopo tutto quello che abbiamo passato..."

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:56 #-#-# translate crowdin hide_and_seek_with_lilian_193a93ac:
msgid "[_cedrick_] Ох, тебя это правда так задело? Не могу сказать, что мне не понравилось проводить с тобой время, но, сама понимаешь, вы, стражницы, слишком долго стоите у нас на пути"
msgstr "[_cedrick_] Ti ha fatto davvero così male? Non posso dire che non mi sia piaciuto passare del tempo con voi, ma, sapete, voi guardie siete state sulla nostra strada per troppo tempo."

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:60 #-#-# translate crowdin hide_and_seek_with_lilian_b6ca52be:
msgid "[_cornelia_] ..И что, служение Фобосу стоит того? Мне казалось, ты был довольно счастлив, когда строил из себя обычного человека"
msgstr "[_cornelia_] ...E servire Phobos ne vale la pena? Pensavo che fossi abbastanza felice quando fingevi di essere un uomo comune."

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:64 #-#-# translate crowdin hide_and_seek_with_lilian_863780f8:
msgid "[_cedrick_] ..Я хорошо притворяюсь, все же это часть моей работы. К тому же, когда я представлял тебя моей служанкой или рабыней – это так же определенно повышало мое настроение"
msgstr "[_cedrick_] Sono bravo a fingere, dopotutto fa parte del mio lavoro. Inoltre, quando ti immaginavo come mia cameriera o schiava, mi sollevava decisamente il morale."

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:68 #-#-# translate crowdin hide_and_seek_with_lilian_fd410bc5:
msgid "[_cornelia_] Ты.. Ты просто чудовище.."
msgstr "[_cornelia_] Sei un... Sei un mostro."

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:72 #-#-# translate crowdin hide_and_seek_with_lilian_7d35c335:
msgid "[_cedrick_] Тебе лучше принять обет, если не хочешь, чтобы с твоей сестрой что-то случилось"
msgstr "[_cedrick_] Faresti meglio a prendere i tuoi voti se non vuoi che accada qualcosa a tua sorella."

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:76 #-#-# translate crowdin hide_and_seek_with_lilian_d01ef303:
msgid "[_cornelia_] Ладно.. Я сделаю это.."
msgstr "[_cornelia_] Ok, ehm... Lo farò..."

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:80 #-#-# translate crowdin hide_and_seek_with_lilian_cc5a5755:
msgid "[_cornelia_] Но перед этим.."
msgstr "[_cornelia_] Ma prima ancora."

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:84 #-#-# translate crowdin hide_and_seek_with_lilian_db1f9a84:
msgid "[_cornelia_] Пообещай и ты, что ни при каких условиях не навредишь моей сестре"
msgstr "[_cornelia_] Promettimi che non farai del male a mia sorella per nessun motivo."

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:88 #-#-# translate crowdin hide_and_seek_with_lilian_49316299:
msgid "[_cedrick_] ..."
msgstr "[_cedrick_] ..."

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:89 #-#-# translate crowdin hide_and_seek_with_lilian_be7b18f4:
msgid "[_cedrick_] (Мда, чем дольше я буду медлить, тем больше здравых идей придет к ней в голову. Так и до вызова стражниц недалеко)"
msgstr "[_cedrick_] (Già, più a lungo procrastino, più idee sensate le verranno in mente. Non manca molto alla convocazione delle guardie)."

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:90 #-#-# translate crowdin hide_and_seek_with_lilian_b3157225:
msgid "[_cedrick_] (Лучше быстро согласиться и не дать ей времени все обдумать)"
msgstr "[_cedrick_] (Meglio accettare in fretta e non darle il tempo di pensarci su)"

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:91 #-#-# translate crowdin hide_and_seek_with_lilian_76ebd6c0:
msgid "[_cedrick_] Я согласен. Приступим к заключению обета"
msgstr "[_cedrick_] Sono d'accordo. Iniziamo le nostre promesse"

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:92 #-#-# translate crowdin hide_and_seek_with_lilian_13c717d6:
msgid "[_cornelia_] ..."
msgstr "[_cornelia_] ..."

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:95 #-#-# translate crowdin hide_and_seek_with_lilian_a20cefa7:
msgid "..."
msgstr "..."

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:98 #-#-# translate crowdin hide_and_seek_with_lilian_0911e14a:
msgid "[_cedrick_] Сначала моя часть обета"
msgstr "[_cedrick_] Prima, la mia parte del voto"

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:99 #-#-# translate crowdin hide_and_seek_with_lilian_0fe2c693:
msgid "[_cedrick_] Я, Лорд Седрик с Меридиана, клянусь ни при каких обстоятельствах не вредить Лилит Хейл"
msgstr "[_cedrick_] Io, Lord Cedric di Meridian, giuro che non farò mai del male a Lilith Hale."

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:100 #-#-# translate crowdin hide_and_seek_with_lilian_cbbde48a:
msgid "[_cedrick_] (Черт.. Теперь не смогу ею воспользоваться.. Но такова плата за поимку стражницы...)"
msgstr "[_cedrick_] (Dannazione... Non posso usarlo ora. Ma questo è il prezzo per catturare il guardiano...)"

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:102 #-#-# translate crowdin hide_and_seek_with_lilian_69e81637:
msgid "[_cedrick_] Клянешься ли ты, Корнелия Хейл, сохранить в секрете то, куда пропала твоя сестра?"
msgstr "[_cedrick_] Lei, Cornelia Hale, giura di mantenere il segreto sul luogo in cui è scomparsa sua sorella?"

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:103 #-#-# translate crowdin hide_and_seek_with_lilian_597c923c:
msgid "[_cornelia_] Клянусь..."
msgstr "[_cornelia_] Lo giuro..."

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:104 #-#-# translate crowdin hide_and_seek_with_lilian_52f31c71:
msgid "[_cedrick_] Клянешься ли ты сохранить тайну моей личности и..."
msgstr "[_cedrick_] Giura di mantenere segreta la mia identità e..."

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:105 #-#-# translate crowdin hide_and_seek_with_lilian_de08f54e:
msgid "[_cornelia_] Клянусь....."
msgstr "[_cornelia_] Giuro....."

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:106 #-#-# translate crowdin hide_and_seek_with_lilian_afee18bd:
msgid "[_cedrick_] Отсосать мой член?"
msgstr "[_cedrick_] Succhiare il mio cazzo?"

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:107 #-#-# translate crowdin hide_and_seek_with_lilian_2ea7c9c4:
msgid "[_cornelia_] Что!?"
msgstr "[_cornelia_] Cosa?"

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:108 #-#-# translate crowdin hide_and_seek_with_lilian_20eed3d7:
msgid "[_cedrick_] Ахах, видела бы ты сейчас свое лицо.. Просто ты такая сексуальная, когда злишься, ничего не могу с собой поделать.."
msgstr "[_cedrick_] Ahhhh, dovresti vedere la tua faccia in questo momento... Sei così sexy quando sei arrabbiato, non posso farci niente..."

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:109 #-#-# translate crowdin hide_and_seek_with_lilian_f6cde6be:
msgid "[_cornelia_] Придурок.. Мы закончили?"
msgstr "[_cornelia_] Stronzo... Abbiamo finito?"

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:110 #-#-# translate crowdin hide_and_seek_with_lilian_f22ba48f:
msgid "[_cedrick_] Ох, ну, раз я сказал это во время заклинания, теперь это стало обязательной частью нашего договора"
msgstr "[_cedrick_] Oh, beh, dato che l'ho detto durante l'incantesimo, ora è diventato parte vincolante del nostro accordo."

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:111 #-#-# translate crowdin hide_and_seek_with_lilian_d128f95a:
msgid "[_cornelia_] Черт.. Да ты издеваешься.."
msgstr "[_cornelia_] Oh, cavolo... Non ci posso credere."

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:112 #-#-# translate crowdin hide_and_seek_with_lilian_d654db38:
msgid "[_cedrick_] Подумай о своей сестре"
msgstr "[_cedrick_] Pensa a tua sorella"

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:113 #-#-# translate crowdin hide_and_seek_with_lilian_158b2bed:
msgid "[_cornelia_] ..Ладно, я клянусь"
msgstr "[_cornelia_] ...OK, lo giuro."

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:115 #-#-# translate crowdin hide_and_seek_with_lilian_e0319752:
msgid "[_cedrick_] Вот и славно.. Мне же не нужно объяснять, что случится, если ты нарушишь непреложный обет?"
msgstr "[_cedrick_] Questo è un bene. Non devo spiegare cosa succede se si infrange il voto, vero?"

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:116 #-#-# translate crowdin hide_and_seek_with_lilian_7bdea869:
msgid "[_cornelia_] Нет, я.. Я знаю.."
msgstr "[_cornelia_] No, io... Lo so..."

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:117 #-#-# translate crowdin hide_and_seek_with_lilian_6b438654:
msgid "[_cedrick_] Тогда, можешь приступать к сосанию моего члена"
msgstr "[_cedrick_] Allora puoi iniziare a succhiare il mio cazzo."

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:119 #-#-# translate crowdin hide_and_seek_with_lilian_6f47266e:
msgid "[_cornelia_] ...."
msgstr "[_cornelia_] ...."

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:122 #-#-# translate crowdin hide_and_seek_with_lilian_a8d1e79b:
msgid "[_cedrick_] Ооооо, да, так-то лучше..."
msgstr "[_cedrick_] Oooh, sì, così va meglio..."

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:123 #-#-# translate crowdin hide_and_seek_with_lilian_d1a409ea:
msgid "[_cornelia_] (...Как такое только могло произойти. А ведь у меня еще были какие-то чувства к этому уроду..)"
msgstr "[_cornelia_] (...Come è potuto accadere. E provavo ancora qualcosa per quel mostro...)."

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:124 #-#-# translate crowdin hide_and_seek_with_lilian_170a53cf:
msgid "[_cedrick_] Хах, держу пари, не так ты себе представляла то, как первый раз будешь мне отсасывать"
msgstr "[_cedrick_] Scommetto che non è così che ti immaginavi la prima volta che mi avresti succhiato il cazzo."

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:125 #-#-# translate crowdin hide_and_seek_with_lilian_638620d4:
msgid "[_cornelia_] П-пошел ты.. Я не представляла с тобой ничего подобного... Т-ты был мне другом.."
msgstr "[_cornelia_] Fanculo... Non avrei mai immaginato una cosa del genere con te... Tu eri mio amico..."

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:126 #-#-# translate crowdin hide_and_seek_with_lilian_60f728ff:
msgid "[_cedrick_] Дааа, интересно, всем ли своим друзьям ты так же старательно отсасывала.."
msgstr "[_cedrick_] Già, mi chiedo se hai succhiato a tutti i tuoi amici quanto hai fatto tu..."

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:127 #-#-# translate crowdin hide_and_seek_with_lilian_384d086a:
msgid "[_cornelia_] (Я делаю это только для того, чтобы все быстрее закончилось...)"
msgstr "[_cornelia_] (Lo faccio solo per far sparire più in fretta...)"

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:129 #-#-# translate crowdin hide_and_seek_with_lilian_33f3afe8:
msgid "*стук в дверь*"
msgstr "# Bussando alla porta #"

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:131 #-#-# translate crowdin hide_and_seek_with_lilian_114d315d:
msgid "[_cedrick_] (ЧЕРТ, НЕУЖЕЛИ ЭТО ЛИЛИТ ВЕРНУЛАСЬ? ЧТО-ТО ЗАБЫЛА В ДОМЕ?)"
msgstr "[_cedrick_] (ACCIDENTI, È TORNATA QUELLA LILITH? HA LASCIATO QUALCOSA A CASA?)."

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:132 #-#-# translate crowdin hide_and_seek_with_lilian_191792cd:
msgid "[_cornelia_] ....."
msgstr "[_cornelia_] ....."

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:134 #-#-# translate crowdin hide_and_seek_with_lilian_33f3afe8_1:
msgid "*стук в дверь*"
msgstr "# Bussando alla porta #"

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:135 #-#-# translate crowdin hide_and_seek_with_lilian_c629e088:
msgid "[_cornelia_] Я-я открою.."
msgstr "[_cornelia_] Ci penso io."

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:136 #-#-# translate crowdin hide_and_seek_with_lilian_0364c501:
msgid "[_cedrick_] Н-нет, стой!"
msgstr "[_cedrick_] N-no, aspetta!"

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:137 #-#-# translate crowdin hide_and_seek_with_lilian_ef423857:
msgid "[_caleb_] Корнелия, это я, Калеб!"
msgstr "[_caleb_] Cornelia, sono io, Caleb!"

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:139 #-#-# translate crowdin hide_and_seek_with_lilian_6f47266e_1:
msgid "[_cornelia_] ...."
msgstr "[_cornelia_] ...."

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:140 #-#-# translate crowdin hide_and_seek_with_lilian_20d2a332:
msgid "[_cedrick_] (Пронесло..)"
msgstr "[_cedrick_] (Buona liberazione...)"

# game/scripts/quests/credulousCornelia/hide_and_seek_with_lilian.rpy:141 #-#-# translate crowdin hide_and_seek_with_lilian_e50288ab:
msgid "[_caleb_] Я знаю, что ты дома! У тебя горит свет!"
