translate italian ql_hayLin_wait_miranda_1912c0d0:

    # miranda "Да, я не говорю, что мне это не нравится"
    miranda "Sì, non sto dicendo che non mi piace"

# game/scripts/quests/haylin/ql_hayLin_wait_miranda.rpy:128
translate italian ql_hayLin_wait_miranda_1efc7dc8:

    # cedrick "Ох.. Ладно, пойдем, не будем заставлять Фобоса ждать"
    cedrick "Oh... Forza, non facciamo aspettare Phobos."

# game/scripts/quests/haylin/ql_hayLin_wait_miranda.rpy:132
translate italian ql_hayLin_wait_miranda_b75cc0a7:

