translate italian ql_hayLin_wait_miranda_e77c57c7:

    # cedrick "Черт, в этом деле лучше тебя нет.."
    cedrick "Diavolo, sei il migliore in questo..."

# game/scripts/quests/haylin/ql_hayLin_wait_miranda.rpy:107
translate italian ql_hayLin_wait_miranda_2ae56faa:

    # miranda "Конечно.. С тобой у меня была куча времени, чтобы потренироваться.."
    miranda "Naturalmente... Con te, ho avuto molto tempo per esercitarmi..."

# game/scripts/quests/haylin/ql_hayLin_wait_miranda.rpy:112
translate italian ql_hayLin_wait_miranda_c3d1d646:

