    # rob "..."
    rob "..."

# game/scripts/quests/searchWitnesses/buy_a_business_suit.rpy:1440
translate italian buy_a_business_suit_45015009:

    # bob "..."
    bob "..."

# game/scripts/quests/searchWitnesses/buy_a_business_suit.rpy:1444
translate italian buy_a_business_suit_4f444cad:

    # rob "Боб, пойди разберись"
    rob "Bob, vai a scoprirlo."

# game/scripts/quests/searchWitnesses/buy_a_business_suit.rpy:1450
translate italian buy_a_business_suit_9c784e47:

    # bob "А почему я?"
    bob "Perché io?"

# game/scripts/quests/searchWitnesses/buy_a_business_suit.rpy:1454
translate italian buy_a_business_suit_f6c1744a:

    # rob "Я в прошлый раз разбирался, твоя очередь"
    rob "Me ne sono occupato l'ultima volta, ora tocca a te."

# game/scripts/quests/searchWitnesses/buy_a_business_suit.rpy:1458
translate italian buy_a_business_suit_f54d0b9b:

    # bob "Ух, ладно, пойду разберусь. Заодно унему цыплят. Ты оставайся тут, Роб"
    bob "Va bene, me ne occupo io. Prendo anche i polli, già che ci sono. Tu resta qui, Rob."

# game/scripts/quests/searchWitnesses/buy_a_business_suit.rpy:1463
translate italian buy_a_business_suit_acea9252:

    # rob "Да, Боб"
    rob "Sì, Bob."

# game/scripts/quests/searchWitnesses/buy_a_business_suit.rpy:1466
translate italian buy_a_business_suit_39ba4e36:

    # centered "{size=37}СПУСТЯ 5 МИНУТ{/size}" with Dissolve(1)
    centered "[withDissolve1]5 MINUTI{/size}SIMMARIO 5 MINUTI{/size}"

# game/scripts/quests/searchWitnesses/buy_a_business_suit.rpy:1470
translate italian buy_a_business_suit_318a6a90:

    # cornelia "Дядя охранник, вы мне не поможете?"
    cornelia "Zio guardiano, mi aiuti?"

# game/scripts/quests/searchWitnesses/buy_a_business_suit.rpy:1474
translate italian buy_a_business_suit_faaae709:

    # rob "Что? Девушка? Уходи отсюда, я работаю"
    rob "Cosa? Una ragazza? Esci di qui, sto lavorando."

# game/scripts/quests/searchWitnesses/buy_a_business_suit.rpy:1478
translate italian buy_a_business_suit_f038cadf:

    # cornelia "Но мне очень нужна ваша помощь в одном деле.."
    cornelia "Ma ho davvero bisogno del tuo aiuto per una cosa..."

# game/scripts/quests/searchWitnesses/buy_a_business_suit.rpy:1482
translate italian buy_a_business_suit_547ca58f:

    # rob "Я. Работаю. Ничто не сдвинет меня с этого места, пока Боб не вернется"
    rob "Я. Lavorare. Niente mi smuoverà finché non tornerà Bob."

