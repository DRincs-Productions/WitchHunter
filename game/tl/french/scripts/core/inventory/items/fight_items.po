msgid "Большое Зелье Здоровья"
msgstr "Grande potion de santé"

# game/scripts/core/inventory/items/fight_items.rpy:2
msgid "Зелье восстанавливает 100 единиц здоровья"
msgstr "La potion restaure 100 unités de santé"

# game/scripts/core/inventory/items/fight_items.rpy:2
msgid "Малое Зелье Здоровья"
msgstr "Petite potion de santé"

# game/scripts/core/inventory/items/fight_items.rpy:2
msgid "Зелье восстанавливает 50 единиц здоровья"
msgstr "La potion restaure 50 unités de santé"

# game/scripts/core/inventory/items/fight_items.rpy:2
msgid "Зелье с ядом"
msgstr "Potion empoisonnée"

# game/scripts/core/inventory/items/fight_items.rpy:2
msgid "Отравляет врага на 5 ходов, каждый ход враг получает 15 урона"
msgstr "Empoisonne l'ennemi pendant 5 tours, à chaque tour l'ennemi subit 15 dégâts."

# game/scripts/core/inventory/items/fight_items.rpy:2
msgid "Сонное зелье"
msgstr "Une potion pour dormir"

# game/scripts/core/inventory/items/fight_items.rpy:2
msgid "Заставляет уснуть врага на 3 хода"
msgstr "Fait dormir l'ennemi pendant 3 tours"

# game/scripts/core/inventory/items/fight_items.rpy:2
msgid "Паутинное зелье"
msgstr "Potion d'araignée"

# game/scripts/core/inventory/items/fight_items.rpy:2
msgid "Замедляет врага на 4 хода"
msgstr "Ralentit l'ennemi pendant 4 tours"

