# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:4
translate french elyon_quarreled_with_her_parents_9522b460:

    # cedrick "Элион, что ты здесь делаешь? Уже довольно поздно..."
    cedrick "Elyon, que faites-vous ici ? Il est bien tard..."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:7
translate french elyon_quarreled_with_her_parents_491cd460:

    # elion "Я-я разругалась с родителями..."
    elion "J'ai eu une dispute avec mes parents..."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:8
translate french elyon_quarreled_with_her_parents_3f882e05:

    # elion "Они сегодня сказали, что завтра утром поедут в командировку по работе на несколько недель.. Я вспылила и сказала, что без хотя бы самого дальнего родственника, они могут и не возвращаться.."
    elion "Ils ont dit aujourd'hui qu'ils partaient en voyage d'affaires demain matin pour quelques semaines... Je me suis emporté et j'ai dit que sans au moins le parent le plus éloigné, ils pourraient ne pas revenir..."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:11
translate french elyon_quarreled_with_her_parents_3e4cda6e:

    # cedrick "Оу.. Жестоко.."
    cedrick "Oh... C'est brutal."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:12
translate french elyon_quarreled_with_her_parents_cc19fffe:

    # cedrick "(Иногда она и вправду слегка напоминает члена семьи Фобоса своим характером)"
    cedrick "(Parfois, elle ressemble en effet légèrement à un membre de la famille Phobos dans son caractère)."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:15
translate french elyon_quarreled_with_her_parents_0e7ff233:

    # elion "Все это дурацкое задание в школе с заполнением родительского древа.. А я же правда ни одного другого родственника не знаю!"
    elion "Tous ces stupides devoirs à l'école pour remplir un arbre généalogique... Et je ne connais vraiment pas d'autres parents !"

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:18
translate french elyon_quarreled_with_her_parents_71c72dd0:

    # elion "Я буду выглядеть как дура с этими тремя фотографиями в этом проклятом родительском древе!"
    elion "Je vais avoir l'air d'un idiot avec ces trois photos dans ce fichu arbre parental !"

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:21
translate french elyon_quarreled_with_her_parents_0314c123:

    # elion "Прости, не хотела на тебе еще срываться..."
    elion "Je suis désolé, je ne voulais pas vous agresser..."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:24
translate french elyon_quarreled_with_her_parents_2e016b64:

    # cedrick "Все в порядке, Элион, я всегда готов тебя выслушать.."
    cedrick "C'est bon, Elyon, je suis toujours prêt à vous écouter..."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:27
translate french elyon_quarreled_with_her_parents_853ad2c4:

    # elion "В общем.. Мне больше некуда идти.. Можно я побуду с тобой?"
    elion "De toute façon... Je n'ai nulle part ailleurs où aller. Je peux rester avec toi ?"

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:30
translate french elyon_quarreled_with_her_parents_a1b3d33e:

    # cedrick "Конечно, я всегда рад твоей компании! Можешь оставаться здесь, пока не помиришься с родителями"
    cedrick "Bien sûr, je suis toujours heureux d'avoir votre compagnie ! Tu peux rester ici jusqu'à ce que tu te réconcilies avec tes parents."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:31
translate french elyon_quarreled_with_her_parents_bcb52893:

    # cedrick "Наверху есть комната, можешь спать там. Она, конечно, не в самом лучшем состоянии пока что, но.."
    cedrick "Il y a une chambre à l'étage, vous pouvez y dormir. Ce n'est pas encore dans le meilleur état, bien sûr, mais..."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:34
translate french elyon_quarreled_with_her_parents_50ca3690:

    # elion "Спасибо тебе, Рик.. Ты, пожалуй, мой самый близкий друг.."
    elion "Merci, Rick. Tu es probablement mon ami le plus proche."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:35
translate french elyon_quarreled_with_her_parents_5dad443a:

    # cedrick "Рад это слышать от тебя, Элион"
    cedrick "Heureux de vous l'entendre dire, Elyon."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:38
translate french elyon_quarreled_with_her_parents_a74af2bc:

    # elion "Я пойду.. Доброй ночи.."
    elion "Je vais partir... Bonne nuit..."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:41
translate french elyon_quarreled_with_her_parents_87a7a9df:

    # cedrick "Доброй.."
    cedrick "Bien..."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:43
translate french elyon_quarreled_with_her_parents_2f4abc0c:

    # cedrick "(Хм, завтра утром уезжают в командировку на несколько недель? Это отличная возможность от них избавиться, пока Элион осталась у меня..)"
    cedrick "(Hmm, je pars demain matin en voyage d'affaires pour quelques semaines ? C'est une bonne occasion de s'en débarrasser pendant qu'Elion reste avec moi...)"

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:44
translate french elyon_quarreled_with_her_parents_68489431:

    # cedrick "Но, как лучше мне с ними поступить.."
    cedrick "Mais, quelle meilleure façon de s'occuper d'eux..."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:49
translate french elyon_quarreled_with_her_parents_79bf5523:

    # cedrick "(Что ж, убивать я их не стану, это было бы слишком жестоко по отношению к Элион. К тому же с этим было бы явно больше хлопот)"
    cedrick "(Bon, je ne vais pas les tuer, ce serait trop cruel pour Elion. De plus, ce serait évidemment plus de problèmes)."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:50
translate french elyon_quarreled_with_her_parents_d7a5f741:

    # cedrick "(Но они в любом случае поплатятся за свою ложь. Я дал им шанс рассказать все в тот вечер, но они продолжили играть в молчанку. Теперь получат результат собственной ошибки)"
    cedrick "(Mais ils allaient payer pour leurs mensonges de toute façon. Je leur ai donné une chance de tout dire cette nuit-là, mais ils ont continué à jouer le jeu du silence. Maintenant, ils obtiendront le résultat de leur propre erreur)."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:51
translate french elyon_quarreled_with_her_parents_83637474:

    # cedrick "(Насколько я помню, Звезда Трибы может в той или иной степени развеять любую магию вокруг себя)"
    cedrick "(Autant que je me souvienne, l'étoile de la tribu peut dissiper toute magie autour d'elle dans une certaine mesure)."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:52
translate french elyon_quarreled_with_her_parents_fd5c69f2:

    # cedrick "(А значит, сила звезды проявится и на родителях Элион, развеяв их маскировку и открыв их уродство во всей красе)"
    cedrick "(Ce qui signifie que le pouvoir de l'étoile se manifestera également sur les parents d'Alyon, dissipant leur déguisement et révélant leur laideur dans toute sa gloire)."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:53
translate french elyon_quarreled_with_her_parents_013beab5:

    # cedrick "(Их иллюзия – довольно слабая магия. Думаю, она развеется в радиусе 50 метров. А дальше..)"
    cedrick "(Leur illusion est une magie assez faible. Je pense qu'elle se dispersera dans un rayon de 50 mètres. Et au-delà de ça...)"

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:54
translate french elyon_quarreled_with_her_parents_65349eef:

    # cedrick "(Хм, поняв их натуру, я понимаю, что они решат сбежать, оставив Элион. Они трусливы, но действительно любят ее)"
    cedrick "(Hm, ayant compris leur nature, je réalise qu'ils vont décider de s'enfuir, laissant Elion derrière eux. Ils sont lâches, mais ils l'aiment vraiment)"

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:55
translate french elyon_quarreled_with_her_parents_97484cf7:

    # cedrick "(Сбежав, они будут надеяться, что люди Фобоса последуют за ними, думая, что Элион сбежит вместе с ними. Пустят их по ложному следу, а сами оставят ее на Земле)"
    cedrick "(En fuyant, ils espèrent que les habitants de Phobos les suivront, pensant qu'Elyon fuira avec eux. Les mener sur une fausse piste et la laisser derrière eux sur Terre)"

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:56
translate french elyon_quarreled_with_her_parents_1b69cf55:

    # cedrick "(И оставят они ее у того, кому она больше всего доверяет. Тому, кто ей ближе всех в этом городе, кто сможет ее защитить. А именно – мне)"
    cedrick "(Et ils la laisseront avec la personne en qui elle a le plus confiance. La personne la plus proche d'elle dans cette ville, celle qui peut la protéger. A savoir moi.)"

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:57
translate french elyon_quarreled_with_her_parents_0fb586fb:

    # cedrick "(Я достаточно долго хорошо притворялся и изучал их, все должно сработать. Но перед этим, мне стоит скрыть свое лицо, чтобы они случайно меня не узнали..)"
    cedrick "(J'ai fait semblant et je les ai assez étudiés, ça devrait marcher. Mais avant cela, je devrais cacher mon visage pour qu'ils ne me reconnaissent pas accidentellement...)"

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:59
translate french elyon_quarreled_with_her_parents_61ecaa91:

    # centered "{size=37}ЭТОЙ ЖЕ НОЧЬЮ В ДОМЕ БРАУНОВ{/size}"
    centered "{CETTE NUIT À LA MAISON BRUNE{/size}"

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:64
translate french elyon_quarreled_with_her_parents_67db9e64:

    # cedrick "(Все готово, нужно лишь вломиться так, чтобы они услышали, и, когда они спустятся, бросить звезду возле их ног. Дальше, магия спадет сама)"
    cedrick "(Tout est prêt, il suffit de s'introduire pour qu'ils puissent entendre, et quand ils descendent, lancez l'étoile à leurs pieds. A partir de là, la magie tombera d'elle-même)"

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:65
translate french elyon_quarreled_with_her_parents_ed60833b:

    # cedrick "(Пожалуй, не стану показываться им на глаза лишний раз. Звезда в любом случае сделает свое дело, мне останется только подождать)"
    cedrick "(Je ne pense pas que je vais leur montrer mon visage à nouveau. L'étoile fera son travail de toute façon, je n'aurai qu'à attendre)."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:67
translate french elyon_quarreled_with_her_parents_78308d23:

    # centered "{size=37}СЕДРИК ВЛАМЫВАЕТСЯ В ДОМ{/size}"
    centered "{SEDRICKS INTO THE HOUSE{/size}"

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:71
translate french elyon_quarreled_with_her_parents_a20cefa7:

    # "..."
    "..."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:73
translate french elyon_quarreled_with_her_parents_d77dd4c5:

    # thomas "Кто здесь, покажись!"
    thomas "Qui est là, montrez-vous !"

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:78
translate french elyon_quarreled_with_her_parents_5d6164b2:

    # eleanor "Дорогой, что случилось.."
    eleanor "Chérie, que s'est-il passé..."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:79
translate french elyon_quarreled_with_her_parents_85047f84:

    # thomas "Черт, сбежал.. Стой, э-это же..."
    thomas "Merde, c'est parti... Attends, c'est..."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:86
translate french elyon_quarreled_with_her_parents_5b12816c:

    # eleanor "О нет, э-это же!"
    eleanor "Oh non, c'est ça !"

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:87
translate french elyon_quarreled_with_her_parents_cde263a5:

    # thomas "Звезда трибы..."
    thomas "Star de la tribu..."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:91
translate french elyon_quarreled_with_her_parents_a2b6f64e:

    # eleanor "А-Альборн, наша маскировка.."
    eleanor "A-Alborn, notre déguisement..."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:97
translate french elyon_quarreled_with_her_parents_22de9657:

    # alborn "Только не это.. И твоя тоже, Мириадель.."
    alborn "Pas ça... Le tien aussi, Myriadelle..."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:103
translate french elyon_quarreled_with_her_parents_8ccb1409:

    # miriadel "Они нашли нас.. Прихвостни Фобоса, спустя столько лет.. Они пришли забрать Элион... Но, почему они ушли.."
    miriadel "Ils nous ont trouvés. Les sbires de Phobos, après toutes ces années... Ils sont venus chercher Elyon... Mais, pourquoi sont-ils partis..."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:109
translate french elyon_quarreled_with_her_parents_d5d69be3:

    # alborn "Звезда Трибы призвана вернуть наследницу престола домой.. Так вот в чем их план.. Ни к чему забирать Элион силой, Звезда сама вернет ее домой.."
    alborn "L'étoile de la tribu est convoquée pour ramener l'héritier du trône chez lui... C'est donc leur plan... Pas besoin de prendre Elyon par la force, l'étoile la ramènera chez elle..."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:115
translate french elyon_quarreled_with_her_parents_fca23978:

    # miriadel "Какое счастье, что она осталась сегодня у Рика.. На таком расстоянии звезда не сможет на нее повлиять.."
    miriadel "Quelle chance qu'elle soit restée chez Rick ce soir. A cette distance, l'étoile ne pourra pas l'affecter..."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:116
translate french elyon_quarreled_with_her_parents_f2ab56be:

    # miriadel "Н-но, что же нам теперь делать.. Звезда развеяла чары, мы не сможем снова превратиться.."
    miriadel "M-mais qu'est-ce qu'on fait maintenant... L'étoile a brisé le sort, nous ne pouvons plus nous retourner..."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:122
translate french elyon_quarreled_with_her_parents_32ff94d4:

    # alborn ".........."
    alborn ".........."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:126
translate french elyon_quarreled_with_her_parents_7b2c18b6:

    # miriadel "Альборн!?"
    miriadel "Alborne !?"

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:132
translate french elyon_quarreled_with_her_parents_b7d878d9:

    # alborn "Мириадель, нам.. Нам придется бежать.."
    alborn "Myriadelle, nous... Nous devons courir..."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:138
translate french elyon_quarreled_with_her_parents_7bac1ffa:

    # miriadel "Н-но как же Элион, мы не можем ее бросить!"
    miriadel "Mais qu'en est-il d'Elyon, nous ne pouvons pas la laisser !"

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:144
translate french elyon_quarreled_with_her_parents_080949c1:

    # alborn "Нам придется!"
    alborn "Nous devons le faire !"

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:145
translate french elyon_quarreled_with_her_parents_05812507:

    # alborn "Если мы заберем ее сейчас, нас попросту схватят. За нами точно уже ведется слежка.. Все, что мы можем – это пустить людей Фобоса по ложному следу.."
    alborn "Si nous l'emmenons maintenant, nous allons simplement nous faire prendre. Nous sommes certainement déjà suivis. Tout ce que nous pouvons faire, c'est envoyer les gens de Phobos à la poursuite d'une oie sauvage..."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:151
translate french elyon_quarreled_with_her_parents_ea276339:

    # alborn "К тому же, посмотри на нас.. В таком виде мы ее только напугаем.. Она даже не поверит, что мы ее родители.."
    alborn "En plus, regarde-nous. On va seulement l'effrayer comme ça. Elle ne croira même pas que nous sommes ses parents."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:157
translate french elyon_quarreled_with_her_parents_1c26305f:

    # miriadel "Но.. Наша девочка.. Она же останется совсем одна..."
    miriadel "Mais... Notre petite fille... Elle sera toute seule..."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:163
translate french elyon_quarreled_with_her_parents_1cec0214:

    # alborn "Нет.. Нет, не одна. Рик - он хороший парень, уверен, он о ней позаботится.."
    alborn "Non... Non, pas seule. Rick - c'est un bon gars, je suis sûr qu'il va s'occuper d'elle..."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:169
translate french elyon_quarreled_with_her_parents_facf3462:

    # miriadel "Да, нам повезло, что мы его встретили, но.. Сможет ли он все понять правильно.. сможет ли Элион понять.. Как мы объясним нашу пропажу?"
    miriadel "Oui, nous avons de la chance de l'avoir rencontré, mais... Est-ce qu'il peut y arriver... est-ce qu'Elion peut y arriver... Comment expliquer notre disparition ?"

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:175
translate french elyon_quarreled_with_her_parents_0d404550:

    # alborn "Нужно оставить записку.. Скажем.. Черт.. Скажем, что нам срочно пришлось уехать по работе в долгую командировку.."
    alborn "On devrait laisser un mot... Dis... Merde... Disons que nous devions partir pour un long voyage d'affaires..."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:181
translate french elyon_quarreled_with_her_parents_518d6b8a:

    # miriadel "Что это все для того, чтобы оплатить долги за дом.. И.. Что нам очень жаль и мы не знаем, сколько точно продлится командировка..."
    miriadel "Qu'il s'agit de rembourser les dettes de la maison... И.. Que nous sommes désolés et que nous ne savons pas exactement combien de temps durera le voyage d'affaires..."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:182
translate french elyon_quarreled_with_her_parents_df6d3ca0:

    # miriadel "Черт, как же так.. Мы даже с ней не попрощались.."
    miriadel "Merde, comment ça se fait... On n'a même pas pu lui dire au revoir..."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:188
translate french elyon_quarreled_with_her_parents_6206af52:

    # alborn "Мы попрощаемся в письме.. Давай, у нас не так много времени.. Как только они поймут, что Элион здесь нет, за нами придут.."
    alborn "On se dira au revoir dans la lettre... Allez, on n'a pas beaucoup de temps. Dès qu'ils verront qu'Elion n'est pas là, ils viendront nous chercher..."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:194
translate french elyon_quarreled_with_her_parents_56b68665:

    # miriadel "Хорошо, сейчас напишу.. И еще, нам ведь лучше забрать эту звезду.."
    miriadel "OK, je vais l'écrire... Aussi, nous ferions mieux d'obtenir cette étoile..."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:200
translate french elyon_quarreled_with_her_parents_2a37d9ed:

    # alborn "Нет! Если мы ее заберем, они поймут, что Элион осталась на Земле и так мы ее оберегаем от влияния звезды"
    alborn "Non ! Si nous l'emmenons, ils réaliseront qu'Elyon est restée sur Terre et c'est ainsi que nous la protégeons de l'influence de l'étoile."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:201
translate french elyon_quarreled_with_her_parents_1ebb0831:

    # alborn "Найдя звезду здесь, они подумают, что мы убежали с Элион прежде, чем она попала под влияние звезды. Так они точно не будут искать Элион на Земле"
    alborn "En trouvant l'étoile ici, ils penseront que nous nous sommes enfuis avec Elyon avant qu'elle ne soit influencée par l'étoile. De cette façon, ils ne chercheront pas Elyon sur Terre."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:207
translate french elyon_quarreled_with_her_parents_01161ecc:

    # miriadel "Да, ты прав.."
    miriadel "Oui, tu as raison..."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:213
translate french elyon_quarreled_with_her_parents_16af99cb:

    # alborn "Мириадель, нам пора уходить.. Я избавлюсь от машины, чтобы еще больше запутать следы"
    alborn "Myriadelle, nous devons partir. Je vais me débarrasser de la voiture pour brouiller encore plus les pistes."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:219
translate french elyon_quarreled_with_her_parents_3ee9fbb3:

    # miriadel "Д-да.. Надеюсь, Рик о ней позаботится.."
    miriadel "Ouais. J'espère que Rick prendra soin d'elle."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:225
translate french elyon_quarreled_with_her_parents_2b69b312:

    # alborn "Уверен, он справится. У них все будет хорошо. Мы не бросаем Элион, а лишь делаем так, чтобы она осталась в безопасности"
    alborn "Je suis sûr qu'il ira bien. Ils iront bien. Nous n'abandonnons pas Elyon, nous nous assurons juste qu'elle reste en sécurité."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:231
translate french elyon_quarreled_with_her_parents_b0cb999d:

    # miriadel "Да.. Это все ради нее.. Теперь нужно оставить письмо у Рика в магазине, прежде чем отправляться на Меридиан"
    miriadel "Oui.. C'est tout sur elle. Maintenant, nous devons laisser la lettre au magasin de Rick avant d'aller à Meridian."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:233
translate french elyon_quarreled_with_her_parents_123f66c0:

    # centered "{size=37}Альборн и Мирабель уходят в сторону магазина Седрика{/size}"
    centered "{Albourn et Mirabel s'éloignent vers la boutique de Cédric{/size}."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:236
translate french elyon_quarreled_with_her_parents_3b487e6b:

    # cedrick "(Что ж, звезда на месте, а значит все прошло согласно моему плану)"
    cedrick "(Eh bien, l'étoile est en place, ce qui signifie que tout s'est passé selon mon plan)"

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:237
translate french elyon_quarreled_with_her_parents_c4d66ab7:

    # cedrick "(Хоть оставлять звезду было слегка рискованно с моей стороны, у них попросту не оставалось других вариантов, кроме как сбежать)"
    cedrick "(Bien que laisser l'étoile derrière moi était un peu risqué de ma part, ils n'avaient simplement pas d'autre choix que de s'enfuir)."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:238
translate french elyon_quarreled_with_her_parents_71f291bf:

    # cedrick "(Им бы следовало попробовать переубедить Элион даже в таком обличии, но, как я уже выяснил до этого – они те еще трусы. А трусам положено бежать)"
    cedrick "(Ils auraient dû essayer de faire changer Elion d'avis, même sous cette apparence, mais comme je l'ai déjà découvert, ce sont des lâches. Et les lâches sont supposés fuir)."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:240
translate french elyon_quarreled_with_her_parents_d9cf32c7:

    # centered "{size=37}Седрик забирает звезду трибы и возвращается к себе в магазин{/size}"
    centered "{Cedric prend l'étoile de la tribu et retourne à sa boutique."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:246
translate french elyon_quarreled_with_her_parents_f435ef5b:

    # cedrick "(Хм, они оставили письмо? Как трогательно.. Но, думаю, мне стоит его подправить, прежде чем отдавать Элион)"
    cedrick "(Hum, ont-ils laissé une lettre ? Comme c'est touchant... Mais je pense que je devrais la peaufiner avant de la donner à Elion)"

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:247
translate french elyon_quarreled_with_her_parents_c8756f1c:

    # cedrick "(Благо, дворцовые интриги научили меня идеально подделывать чужой подчерк в записках, хах)"
    cedrick "(Heureusement, l'intrigue de palais m'a appris à falsifier parfaitement l'écriture de quelqu'un d'autre dans les notes, hein)."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:249
translate french elyon_quarreled_with_her_parents_3ae3404e:

    # centered "{size=37}Утром на следующий день{/size}"
    centered "{Le lendemain matin..."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:254
translate french elyon_quarreled_with_her_parents_bc1b050a:

    # cedrick "Элион, доброе утро"
    cedrick "Elyon, bonjour."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:257
translate french elyon_quarreled_with_her_parents_f7e4233e:

    # elion "Доброе утро, Рик"
    elion "Bonjour, Rick."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:258
translate french elyon_quarreled_with_her_parents_1377545a:

    # cedrick "Эм, тут вчера ночью кто-то пришел и оставил письмо, судя по конверту оно оставлено тебе"
    cedrick "Hum, quelqu'un est venu ici la nuit dernière et a laissé une lettre, l'enveloppe dit que c'est pour vous."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:261
translate french elyon_quarreled_with_her_parents_24673d32:

    # elion "Мне? Кто бы это мог быть.. А что в письме написано?"
    elion "Moi ? Qui ça peut être ? Que dit la lettre ?"

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:262
translate french elyon_quarreled_with_her_parents_30defff2:

    # cedrick "Я еще не читал, это же чужое письмо. Надеялся, что ты мне расскажешь"
    cedrick "Je ne l'ai pas encore lu, c'est la lettre de quelqu'un d'autre. J'espérais que vous pourriez me dire"

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:265
translate french elyon_quarreled_with_her_parents_7b78d77b:

    # elion "Так, дай-ка почитать.."
    elion "Maintenant, laissez-moi lire ceci..."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:268
translate french elyon_quarreled_with_her_parents_97c78e07:

    # elion "Дорогая Элион.. Это же почерк мамы"
    elion "Cher Elyon... C'est l'écriture de maman."

# game/scripts/quests/elyonQuarreledWithHerParents/elyon_quarreled_with_her_parents.rpy:269
translate french elyon_quarreled_with_her_parents_038ab544:

    # elion "....."
    elion "....."

