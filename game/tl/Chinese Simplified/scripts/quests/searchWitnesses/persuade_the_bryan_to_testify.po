# TODO: Translation updated at 2023-07-25 15:55 #|#|# # game/scripts/quests/searchWitnesses/persuade_the_bryan_to_testify.rpy:5 #-#-# translate crowdin persuade_the_bryan_to_testify_cfdea0de:
msgid "[_cedrick_] Здравствуй, Брайан. Смотрю ты немного подкачался, пахая на рудниках. Хотя нет, показалось"
msgstr ""

# game/scripts/quests/searchWitnesses/persuade_the_bryan_to_testify.rpy:8 #-#-# translate crowdin persuade_the_bryan_to_testify_716bc1bc:
msgid "[_brayan_] Т-ты, ублюдок! Что ты здесь делаешь!?"
msgstr ""

# game/scripts/quests/searchWitnesses/persuade_the_bryan_to_testify.rpy:9 #-#-# translate crowdin persuade_the_bryan_to_testify_bafac30f:
msgid "[_cedrick_] Ого, у тебя даже силы злиться еще остались? Уже забыл, как жалобно скулил, когда я тебя сюда забросил"
msgstr ""

# game/scripts/quests/searchWitnesses/persuade_the_bryan_to_testify.rpy:13 #-#-# translate crowdin persuade_the_bryan_to_testify_bbf10c04:
msgid "[_brayan_] Ч-что тебе нужно?.."
msgstr ""

# game/scripts/quests/searchWitnesses/persuade_the_bryan_to_testify.rpy:15 #-#-# translate crowdin persuade_the_bryan_to_testify_be9a0cf4:
msgid "[_cedrick_] Сотрудничество"
msgstr ""

# game/scripts/quests/searchWitnesses/persuade_the_bryan_to_testify.rpy:19 #-#-# translate crowdin persuade_the_bryan_to_testify_d87cbb69:
msgid "[_brayan_] Чтобы я? С тобой!? После всего.. Н-ни за что! Лучше убей меня"
msgstr ""

# game/scripts/quests/searchWitnesses/persuade_the_bryan_to_testify.rpy:20 #-#-# translate crowdin persuade_the_bryan_to_testify_18bd77db:
msgid "[_cedrick_] Ожидаемо. Хорошо, смотри кто у меня здесь"
msgstr ""

# game/scripts/quests/searchWitnesses/persuade_the_bryan_to_testify.rpy:25 #-#-# translate crowdin persuade_the_bryan_to_testify_c3cf8b4d:
msgid "[_brayan_] А-алхими!?"
msgstr ""

# game/scripts/quests/searchWitnesses/persuade_the_bryan_to_testify.rpy:29 #-#-# translate crowdin persuade_the_bryan_to_testify_950bdcf9:
msgid "[_alchemy_] П-привет, Брайан.."
msgstr ""

# game/scripts/quests/searchWitnesses/persuade_the_bryan_to_testify.rpy:35 #-#-# translate crowdin persuade_the_bryan_to_testify_a11ac845:
msgid "[_brayan_] Урод! Отпусти ее, она здесь ни при чем!!"
msgstr ""

# game/scripts/quests/searchWitnesses/persuade_the_bryan_to_testify.rpy:36 #-#-# translate crowdin persuade_the_bryan_to_testify_4c57bfc3:
msgid "[_cedrick_] Повторюсь, Брайан, мне нужно, чтобы ты со мной сотрудничал. Я не хочу жертв или увечий — это не особо продуктивно"
msgstr ""

# game/scripts/quests/searchWitnesses/persuade_the_bryan_to_testify.rpy:37 #-#-# translate crowdin persuade_the_bryan_to_testify_81716cb1:
msgid "[_cedrick_] Но, чтобы ты стал более сговорчивым, я привел сюда твою сестру. Чтобы ты понял, что даже если ты сам умрешь, на этом все не закончится"
msgstr ""

# game/scripts/quests/searchWitnesses/persuade_the_bryan_to_testify.rpy:38 #-#-# translate crowdin persuade_the_bryan_to_testify_ed9c93cf:
msgid "[_cedrick_] Твои необдуманные слова и действия могут навредить твоим самым близким людям"
msgstr ""

# game/scripts/quests/searchWitnesses/persuade_the_bryan_to_testify.rpy:39 #-#-# translate crowdin persuade_the_bryan_to_testify_76931f2f:
msgid "[_brayan_] ..."
msgstr ""

# game/scripts/quests/searchWitnesses/persuade_the_bryan_to_testify.rpy:40 #-#-# translate crowdin persuade_the_bryan_to_testify_6df3e9f3:
msgid "[_cedrick_] Мы поняли друг друга?"
msgstr ""

# game/scripts/quests/searchWitnesses/persuade_the_bryan_to_testify.rpy:44 #-#-# translate crowdin persuade_the_bryan_to_testify_9984bce5:
msgid "[_brayan_] Что ты хочешь, чтобы я сделал?.."
msgstr ""

# game/scripts/quests/searchWitnesses/persuade_the_bryan_to_testify.rpy:45 #-#-# translate crowdin persuade_the_bryan_to_testify_e24e05a2:
msgid "[_cedrick_] Мне нужно, чтобы ты выступил в суде, в качестве свидетеля на моей стороне"
msgstr ""

# game/scripts/quests/searchWitnesses/persuade_the_bryan_to_testify.rpy:49 #-#-# translate crowdin persuade_the_bryan_to_testify_76931f2f_1:
msgid "[_brayan_] ..."
msgstr ""

# game/scripts/quests/searchWitnesses/persuade_the_bryan_to_testify.rpy:50 #-#-# translate crowdin persuade_the_bryan_to_testify_a2a8062e:
msgid "[_cedrick_] Сыграл роль похищенной жертвы, коей ты и являешься. Только похитил тебя другой человек. Понимаешь?"
msgstr ""

# game/scripts/quests/searchWitnesses/persuade_the_bryan_to_testify.rpy:54 #-#-# translate crowdin persuade_the_bryan_to_testify_6f0488e4:
msgid "[_brayan_] Хочешь, чтобы я оправдал тебя? Чтобы тебе все сошло с рук?"
msgstr ""

# game/scripts/quests/searchWitnesses/persuade_the_bryan_to_testify.rpy:55 #-#-# translate crowdin persuade_the_bryan_to_testify_c9b5b577:
msgid "[_cedrick_] Да. И ты поможешь мне в этом"
msgstr ""

# game/scripts/quests/searchWitnesses/persuade_the_bryan_to_testify.rpy:59 #-#-# translate crowdin persuade_the_bryan_to_testify_86ccb8ca:
msgid "[_brayan_] Что, вернешь меня на Землю?"
msgstr ""

# game/scripts/quests/searchWitnesses/persuade_the_bryan_to_testify.rpy:63 #-#-# translate crowdin persuade_the_bryan_to_testify_d42c8496:
msgid "[_cedrick_] На время. Не переживай, и на тебе и на Алхими будет заклинание, которое взорвет вам головы, если вы не вернетесь на Меридиан через 24 часа"
msgstr ""

# game/scripts/quests/searchWitnesses/persuade_the_bryan_to_testify.rpy:67 #-#-# translate crowdin persuade_the_bryan_to_testify_9443def3:
msgid "[_alchemy_] Ч-что!?"
msgstr ""

# game/scripts/quests/searchWitnesses/persuade_the_bryan_to_testify.rpy:68 #-#-# translate crowdin persuade_the_bryan_to_testify_26ae8404:
msgid "[_cedrick_] Кхм.."
msgstr ""

# game/scripts/quests/searchWitnesses/persuade_the_bryan_to_testify.rpy:74 #-#-# translate crowdin persuade_the_bryan_to_testify_041f2343:
msgid "[_alchemy_] А-а.. Брайан, он не шутит!"
msgstr ""

# game/scripts/quests/searchWitnesses/persuade_the_bryan_to_testify.rpy:78 #-#-# translate crowdin persuade_the_bryan_to_testify_c86da72a:
msgid "[_brayan_] Ты, чудовище.."
msgstr ""

# game/scripts/quests/searchWitnesses/persuade_the_bryan_to_testify.rpy:82 #-#-# translate crowdin persuade_the_bryan_to_testify_183c1cc9:
msgid "[_cedrick_] Взамен, по твоему возвращению, ты больше не будешь работать в шахте. Я отправлю тебя в темницу"
msgstr ""

# game/scripts/quests/searchWitnesses/persuade_the_bryan_to_testify.rpy:83 #-#-# translate crowdin persuade_the_bryan_to_testify_c33f9bca:
msgid "[_cedrick_] Будешь просто валяться в четырех стенах и есть три раза в день. При чем еда будет лучше, чем то, что дают тебе здесь"
msgstr ""

# game/scripts/quests/searchWitnesses/persuade_the_bryan_to_testify.rpy:87 #-#-# translate crowdin persuade_the_bryan_to_testify_63dee156:
msgid "[_brayan_] Хах, кнут и пряник, да?"
msgstr ""

# game/scripts/quests/searchWitnesses/persuade_the_bryan_to_testify.rpy:88 #-#-# translate crowdin persuade_the_bryan_to_testify_e17abf10:
msgid "[_cedrick_] Именно"
msgstr ""

# game/scripts/quests/searchWitnesses/persuade_the_bryan_to_testify.rpy:89 #-#-# translate crowdin persuade_the_bryan_to_testify_2328085a:
msgid "[_brayan_] Ладно.. Я согласен.. Только не тронь Алхими"
msgstr ""

# game/scripts/quests/searchWitnesses/persuade_the_bryan_to_testify.rpy:95 #-#-# translate crowdin persuade_the_bryan_to_testify_830153de:
msgid "[_cedrick_] Отлично. Я вернусь за тобой, как только придет время"
msgstr ""

# game/scripts/quests/searchWitnesses/persuade_the_bryan_to_testify.rpy:96 #-#-# translate crowdin persuade_the_bryan_to_testify_76931f2f_2:
msgid "[_brayan_] ..."
msgstr ""

# game/scripts/quests/searchWitnesses/persuade_the_bryan_to_testify.rpy:100 #-#-# translate crowdin persuade_the_bryan_to_testify_6310e28c:
msgid "[_cedrick_] Что ж, Брайн и Алхими есть. На очереди Корнелия"
msgstr ""

