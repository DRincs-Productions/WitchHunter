# game/scripts/quests/growingUpLilian/sitWithLilian2.rpy:698 #-#-# translate crowdin lilith_no_5a4016ea:
msgid "[_centered_] {size=37}ЛИЛИТ БЫСТРО СОБИРАЕТ ВЕЩИ В РЮКЗАК, ПРИНИМАЕТ КУЧУ ЛЕКАРСТВ И ВЫБЕГАЕТ ИЗ КВАРТИРЫ{/size} [withDissolve1]"
