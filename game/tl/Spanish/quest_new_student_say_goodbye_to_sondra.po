msgstr "[_cedrick_] Oh.."

# game/scripts/quests/newStudent/quest_new_student_say_goodbye_to_sondra.rpy:85 #-#-# translate crowdin quest_new_student_say_goodbye_to_sondra_0595fa7c:
msgid "[_cedrick_] всегда хотел сделать это на парте... Готовься к прощальному подарку"
msgstr "[_cedrick_] Siempre quise hacerlo en el escritorio... Prepárate para un regalo de despedida"

