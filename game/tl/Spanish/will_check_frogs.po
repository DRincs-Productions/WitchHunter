# TODO: Translation updated at 2022-12-22 01:40 #|#|# # game/scripts/quests/will/will_check_frogs.rpy:13 #-#-# translate crowdin will_check_frogs_7fd3ab08:
msgid "[_will_] Привет, Рик, верно?"
msgstr "[_will_] Oye, Rick, ¿cierto?"

# game/scripts/quests/will/will_check_frogs.rpy:14 #-#-# translate crowdin will_check_frogs_08b61c66:
msgid "[_cedrick_sprite_] Да?"
msgstr "[_cedrick_sprite_] ¿Sí?"

# game/scripts/quests/will/will_check_frogs.rpy:20 #-#-# translate crowdin will_check_frogs_48044e8b:
msgid "[_will_] Послушай, в прошлый раз я заметила что у вас помимо книг продаются различные коллекционные игрушки, и подумала..."
msgstr "[_will_] Mira, la última vez me di cuenta de que vendías varios juguetes de colección además de libros, y pensé..."

# game/scripts/quests/will/will_check_frogs.rpy:26 #-#-# translate crowdin will_check_frogs_dc1b4173:
msgid "[_will_] У вас случайно нет коллекционной лягушки из Cut the Rope?"
msgstr "[_will_] ¿Por casualidad tienes una rana coleccionable de Cut the Rope?"

# game/scripts/quests/will/will_check_frogs.rpy:27 #-#-# translate crowdin will_check_frogs_0e982999:
msgid "[_cedrick_sprite_] Cur the Rope?"
msgstr "[_cedrick_sprite_] ¿Cur the Rope?"

# game/scripts/quests/will/will_check_frogs.rpy:28 #-#-# translate crowdin will_check_frogs_4f5f384a:
msgid "[_will_] Ну как бы объяснить, это популярная мобильная игра"
msgstr "[_will_] ¿Cómo explicarlo?, es un popular juego para móviles"

# game/scripts/quests/will/will_check_frogs.rpy:29 #-#-# translate crowdin will_check_frogs_324e887b:
msgid "[_cedrick_sprite_] Понятно, но боюсь что у нас нет в наличии подобных игрушек"
msgstr "[_cedrick_sprite_] Ya veo, pero me temo que no tenemos juguetes como ese en stock"

# game/scripts/quests/will/will_check_frogs.rpy:35 #-#-# translate crowdin will_check_frogs_a5d08a40:
msgid "[_will_] Очень жаль, их нет не где в нашем городе"
msgstr "[_will_] Lástima que no estén disponibles en ninguna parte de nuestra ciudad"

# game/scripts/quests/will/will_check_frogs.rpy:36 #-#-# translate crowdin will_check_frogs_6977534a:
msgid "[_will_] И почему мы переехали в такое захолустье.."
msgstr "[_will_] ¿Por qué nos mudamos a un lugar tan atrasado?.."

# game/scripts/quests/will/will_check_frogs.rpy:37 #-#-# translate crowdin will_check_frogs_d821f42d:
msgid "[_cedrick_sprite_] Кстати говоря, я как раз планировал расширять ассортимент магазина и раз она так популярна, то почему бы не заказать"
msgstr "[_cedrick_sprite_] Hablando de eso, estaba planeando ampliar el stock de la tienda y, ya que es tan popular, ¿por qué no pedirlos?"

# game/scripts/quests/will/will_check_frogs.rpy:43 #-#-# translate crowdin will_check_frogs_18fb1631:
msgid "[_will_] Это было бы просто замечательно, я была бы очень благодарна"
msgstr "[_will_] Sería estupendo, te lo agradecería mucho"

# game/scripts/quests/will/will_check_frogs.rpy:44 #-#-# translate crowdin will_check_frogs_7f604cf9:
msgid "[_cedrick_sprite_] Ты можешь оставить свой адрес и как только игрушка придет, я или Элион доставим ее тебе."
msgstr "[_cedrick_sprite_] Puedes dejar tu dirección y en cuanto llegue el juguete, Elyon o yo te lo entregaremos."

# game/scripts/quests/will/will_check_frogs.rpy:50 #-#-# translate crowdin will_check_frogs_7f2b1c1d:
msgid "[_will_] Спасибо, но лучше я сама зайду, книжный магазин находится как раз по пути в школу"
msgstr "[_will_] Gracias, pero será mejor que venga yo, la librería está de camino a la escuela"

# game/scripts/quests/will/will_check_frogs.rpy:51 #-#-# translate crowdin will_check_frogs_510c9b12:
msgid "[_cedrick_sprite_] (... Жаль неплохо было бы узнать где она живет..)"
msgstr "[_cedrick_sprite_] (... Lástima, sería bueno saber dónde vive...)"

# game/scripts/quests/will/will_check_frogs.rpy:52 #-#-# translate crowdin will_check_frogs_a120d588:
msgid "[_cedrick_sprite_] Хорошо, думаю они придут в течении недели"
msgstr "[_cedrick_sprite_] Bien, creo que estarán aquí en una semana"

# game/scripts/quests/will/will_check_frogs.rpy:53 #-#-# translate crowdin will_check_frogs_d12a67b1:
msgid "[_will_] Еще раз спасибо, ну я пошла"
msgstr "[_will_] Gracias de nuevo, bueno, me voy"

# game/scripts/quests/will/will_check_frogs.rpy:54 #-#-# translate crowdin will_check_frogs_9473ab28:
msgid "[_cedrick_sprite_] (... Мне нужен телефон чтобы сделать заказ)"
msgstr "[_cedrick_sprite_] (... Necesito el teléfono para hacer el pedido)"

