# game/scripts/quests/preparingAlchemyForPrincePhobos/reiter_s_attempt_to_alchemy.rpy:684 #-#-# translate crowdin reiter_s_attempt_to_alchemy_failed41_659864c7:
msgid "[_phobos_] Тогда почему же она голая, Седрик?"
msgstr "[_phobos_] ¿Entonces por qué está desnuda, Cedric?"

# game/scripts/quests/preparingAlchemyForPrincePhobos/reiter_s_attempt_to_alchemy.rpy:688 #-#-# translate crowdin reiter_s_attempt_to_alchemy_failed41_aed618cb:
msgid "[_phobos_] Ты что, решил мне солгать!? Да как ты посмел!"
