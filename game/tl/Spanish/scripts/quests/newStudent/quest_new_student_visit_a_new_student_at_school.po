# game/scripts/quests/newStudent/quest_new_student_visit_a_new_student_at_school.rpy:81 #-#-# translate crowdin quest_new_student_visit_a_new_student_at_school_bba4cc1d:
msgid "[_cedrick_] (Что-то она там долго...)"
msgstr "[_cedrick_] (Algo le está llevando mucho tiempo...)"

# game/scripts/quests/newStudent/quest_new_student_visit_a_new_student_at_school.rpy:82 #-#-# translate crowdin quest_new_student_visit_a_new_student_at_school_23137bfe:
