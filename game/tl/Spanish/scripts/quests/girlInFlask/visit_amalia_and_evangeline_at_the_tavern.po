msgstr "[_evangelin_] ¡Cállate!"

# game/scripts/quests/girlInFlask/visit_amalia_and_evangeline_at_the_tavern.rpy:273 #-#-# translate crowdin visit_amalia_and_evangeline_at_the_tavern_33e6d964:
msgid "[_cedrick_] Признайтесь, вам это нравится"
msgstr "[_cedrick_] Admítelo, te gusta"

# game/scripts/quests/girlInFlask/visit_amalia_and_evangeline_at_the_tavern.rpy:276 #-#-# translate crowdin visit_amalia_and_evangeline_at_the_tavern_b31a485f:
msgid "[_amalia_] Молчи! М-м-м..."
msgstr "[_amalia_] ¡Silencio! Um-m-m..."

# game/scripts/quests/girlInFlask/visit_amalia_and_evangeline_at_the_tavern.rpy:279 #-#-# translate crowdin visit_amalia_and_evangeline_at_the_tavern_398db1ea:
