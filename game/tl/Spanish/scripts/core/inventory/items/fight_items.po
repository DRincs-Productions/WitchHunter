msgstr "Gran poción de salud"

# game/scripts/core/inventory/items/fight_items.rpy:2
msgid "Зелье восстанавливает 100 единиц здоровья"
msgstr "Recupera 100 puntos de salud"

# game/scripts/core/inventory/items/fight_items.rpy:2
