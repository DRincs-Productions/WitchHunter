msgid "Играть на золото"
msgstr "Jugar por oro"

# game/scripts/meridian/tavern/labels/herald.rpy:2
msgid "Попросить ингредиент для зелья"
msgstr "Pide un ingrediente para una poción"

# game/scripts/meridian/tavern/labels/herald.rpy:2
msgid "Сыграть в покер на ингредиент"
msgstr "Jugar al póquer por un ingrediente"

# game/scripts/meridian/tavern/labels/herald.rpy:2
msgid "Играть на доску"
msgstr "Jugar en el tablero"

# game/scripts/meridian/tavern/labels/herald.rpy:2
msgid "назад"
msgstr "volver"

# game/scripts/meridian/tavern/labels/herald.rpy:6
