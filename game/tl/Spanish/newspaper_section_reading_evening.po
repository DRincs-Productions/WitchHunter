# game/scripts/quests/newspaperSection/newspaper_section_reading_evening.rpy:252 #-#-# translate crowdin newspaper_section_reading_evening_591c9b56:
msgid "[_irma_] Ирма Леир оказалась... ЧТО!?"
msgstr "[_irma_] Irma Leir resultó ser... ¿¡QUÉ!?"

# game/scripts/quests/newspaperSection/newspaper_section_reading_evening.rpy:253 #-#-# translate crowdin newspaper_section_reading_evening_adeb93eb:
