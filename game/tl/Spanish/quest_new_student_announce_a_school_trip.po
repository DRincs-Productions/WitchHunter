msgstr "[_cedrick_] ¿Sondra Schmid?"

# game/scripts/quests/newStudent/quest_new_student_announce_a_school_trip.rpy:26 #-#-# translate crowdin quest_new_student_announce_a_school_trip_da595a72:
msgid "[_sondra_] Разумеется поеду.."
msgstr "[_sondra_] Por supuesto que iré.."

# game/scripts/quests/newStudent/quest_new_student_announce_a_school_trip.rpy:27 #-#-# translate crowdin quest_new_student_announce_a_school_trip_839950a7:
msgid "[_cedrick_] Мэтт Олсен?"
msgstr "[_cedrick_] ¿Matt Olsen?"

# game/scripts/quests/newStudent/quest_new_student_announce_a_school_trip.rpy:28 #-#-# translate crowdin quest_new_student_announce_a_school_trip_db883e25:
msgid "[_matt_] Да, я поеду.."
msgstr "[_matt_] Sí, iré.."

# game/scripts/quests/newStudent/quest_new_student_announce_a_school_trip.rpy:29 #-#-# translate crowdin quest_new_student_announce_a_school_trip_98561895:
msgid "[_cedrick_] Эндрю Хорнби?"
msgstr "[_cedrick_] ¿Andrew Hornby?"

# game/scripts/quests/newStudent/quest_new_student_announce_a_school_trip.rpy:30 #-#-# translate crowdin quest_new_student_announce_a_school_trip_4b13fe84:
